{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": false,
    "max-classes-per-file": false,
    "jsx-boolean-value": false,
    "interface-name": false,
    "prefer-const": false,
    "no-string-literal": false,
    "no-console": false,
    "radix": false,
    "jsx-no-lambda": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}